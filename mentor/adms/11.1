#%Module1.0#####################################################################

# Local variables
set MODULE "adms"
set VERSION "11.1"
set BASE "/soft64/mentor/ferramentas/${MODULE}/${VERSION}"

# License
setenv MGLS_LICENSE_FILE 27005@kriti.inf.pucrs.br

# Extra variables
setenv mgc_ams $BASE

# Simple user information
if { [ module-info mode load ] } {
	puts stderr "\n\t${MODULE} ${VERSION} module was loaded\n"
}
if { [ module-info mode remove ] } {
	puts stderr "\n\t${MODULE} ${VERSION} module was removed\n"
}

# Help 
module-whatis "Matheus/Julian descrever a ferramentas. Run 'systest all' to test the tool"

# Binaries
prepend-path PATH "${BASE}/bin:${BASE}/modeltech/bin"

setenv MGC_AMS_HOME "${BASE}"

